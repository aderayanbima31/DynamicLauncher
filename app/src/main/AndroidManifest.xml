<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.derayanbimaa.dynamiclauncher">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_dana_normal"
        android:label="DANA Dynamic"
        android:roundIcon="@mipmap/ic_launcher_dana_normal_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DynamicLauncher">
        <activity android:name=".MainActivity" />
        <activity-alias
            android:name="DynamicLauncherOne"
            android:enabled="true"
            android:icon="@mipmap/ic_launcher_dana_normal"
            android:label="DANA"
            android:targetActivity=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity-alias>

        <activity-alias
            android:name="DynamicLauncherTwo"
            android:enabled="false"
            android:icon="@mipmap/ic_launcher_dana_ramadhan"
            android:label="DANA Ramadhan"
            android:targetActivity=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity-alias>

        <activity-alias
            android:name="DynamicLauncherThree"
            android:enabled="false"
            android:icon="@mipmap/ic_launcher_dana_agustusan"
            android:label="DANA Agustusan"
            android:targetActivity=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity-alias>
    </application>

</manifest>